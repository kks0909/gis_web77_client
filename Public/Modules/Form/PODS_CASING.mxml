<?xml version="1.0" encoding="UTF-8"?>
<DATA_EDIT_FORM xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:picPutton="ru.corelight.view.controls.*" xmlns:panel="ru.corelight.grids.buttons.*" xmlns:controls="view.controls.*"
                xmlns="view.data.*"
                title="Защитные кожухи (футляры)"
                width="760"
                ext_data_id="PODS_EXPORT.xml#PODS_CASING"
                excelTemplate="PODS_CASING">
	  <dataProvider>PODS_SEM.xml</dataProvider>
   <panel:buttons>
     <picPutton:PanelButton source="../images/catalog.png"
           tooltip="Прикрепленные документы / комментарии"
           clickEvent="DocsEvent" />
     <picPutton:PanelButton source="../images/save.png"
           tooltip="Сохранить"
           clickEvent="SaveEvent" />
     <picPutton:PanelButton source="../images/undo.png"
           tooltip="Отменить изменения"
           clickEvent="UndoEvent" />
     <picPutton:PanelButton source="../images/delete.png"
           tooltip="Удалить"
           clickEvent="DeleteEvent"/>
     <picPutton:PanelButton source="../images/exportNAV.png"
           tooltip="Экспорт в GPX, KML"
           clickEvent="ExportGpxEvent" />
     <picPutton:PanelButton source="../images/showOnMap.png"
           tooltip="Показать на карте"
           clickEvent="ShowOnMapEvent" />
     <picPutton:PanelButton source="../images/find_path.png"
           tooltip="Построение маршрута"
           clickEvent="FindRouteEvent"/>
     <picPutton:PanelButton source="../images/bufferZone.png"
           tooltip="Построить буферную зону"
           clickEvent="BufferCustomEvent"/>
     <picPutton:PanelButton source="../images/excel.png"
           tooltip="Экспорт в Excel"
           clickEvent="ExportToExcelEvent" />
     <picPutton:PanelButton source="../images/back.png"
           tooltip="Вернуться к предыдущему объекту"
           clickEvent="BackEvent"/>
     <picPutton:PanelButton source="../images/forward.png"
           tooltip="Отменить возврат к предыдущему объекту"
           clickEvent="ForwardEvent"/>
   </panel:buttons>
	  <mx:VBox width="760">
		    <mx:HBox percentWidth="100">
			      <mx:VBox percentWidth="50">
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Описание" width="140"/>
					          <mx:TextInput id="DESCRIPTION" width="225" maxChars="50" height="40"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Компания-изготовитель" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="MANUFACTURER_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_CASING_MANUFACTURER_CL"/>
						            <mx:Button id="BTN_LIST_MANUFACTURER_CL" selectedField="PODS_CASING_MANUFACTURER_CL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Местоположение трубного производства" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="PIPE_MILL_LOCATION_GCL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_PIPE_MILL_LOCATION_GCL"/>
						            <mx:Button id="BTN_LIST_PIPE_MILL_LOCATION_GCL"
                             selectedField="PODS_PIPE_MILL_LOCATION_GCL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Материал" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="MATERIAL_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_CASING_MATERIAL_CL"/>
						            <mx:Button id="BTN_LIST_MATERIAL_CL" selectedField="PODS_CASING_MATERIAL_CL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Марка стали" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="PIPE_GRADE_GCL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_PIPE_GRADE_GCL"/>
						            <mx:Button id="BTN_LIST_PIPE_GRADE_GCL" selectedField="PODS_PIPE_GRADE_GCL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Предел текучести стали, Н/мм2" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="SMYS_GCL" width="110" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_SMYS_GCL"/>
						            <mx:Button id="BTN_LIST_SMYS_GCL" selectedField="PODS_SMYS_GCL" enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Технические требования" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="PIPE_SPECIFICATION_GCL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_PIPE_SPECIFICATION_GCL"/>
						            <mx:Button id="BTN_LIST_PIPE_SPECIFICATION_GCL"
                             selectedField="PODS_PIPE_SPECIFICATION_GCL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Номинальный диаметр/ толщина стенки, мм" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="NOMINAL_DIAMETER_GCL" width="80" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_NOMINAL_DIAMETER_GCL"/>
						            <mx:Button id="BTN_LIST_NOMINAL_DIAMETER_GCL" selectedField="PODS_NOMINAL_DIAMETER_GCL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="NOMINAL_WALL_THICKNESS_GCL" width="80" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_NOMINAL_WALL_THICKNESS_GCL"/>
						            <mx:Button id="BTN_LIST_NOMINAL_WALL_THICKNESS_GCL"
                             selectedField="PODS_NOMINAL_WALL_THICKNESS_GCL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
			      </mx:VBox>
			      <mx:VBox percentWidth="50">
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Длина кожуха, м" width="140"/>
					          <controls:NumberInput id="LENGTH" width="30" maxChars="5" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Вытяжные свечи" width="140"/>
					          <mx:CheckBox id="VENTED_INDICATOR_LF" width="16" height="16"/>
					          <mx:Text textAlign="right" text="Количество вытяжных свечей" width="120"/>
					          <controls:NumberInput id="QUANTITY_OF_VENTS" width="30" maxChars="5" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Дата изготовления/ установки-монтажа" width="140"/>
					          <mx:DateField id="DATE_MANUFACTURED" width="96" height="20" formatString="DD.MM.YYYY"/>
					          <mx:DateField id="DATE_INSTALLED" width="96" height="20" formatString="DD.MM.YYYY"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Тип перехода" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="CROSSING_TYPE_GCL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_CROSSING_TYPE_GCL"/>
						            <mx:Button id="BTN_LIST_CROSSING_TYPE_GCL" selectedField="PODS_CROSSING_TYPE_GCL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Тип изоляции" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="INSULATOR_TYPE_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_CASING_INSULATOR_TYPE_CL"/>
						            <mx:Button id="BTN_LIST_INSULATOR_TYPE_CL" selectedField="PODS_CASING_INSULATOR_TYPE_CL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Тип уплотнения" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="SEAL_TYPE_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_CASING_SEAL_TYPE_CL"/>
						            <mx:Button id="BTN_LIST_SEAL_TYPE_CL" selectedField="PODS_CASING_SEAL_TYPE_CL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Комментарии" width="140"/>
					          <mx:TextInput id="COMMENTS" width="225" maxChars="255" height="40"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100" visible="{CONFIG.SHOW_SYSTEM_INFO}">
					          <mx:Text textAlign="right" text="Источник информации" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="SOURCE_GCL" enabled="false" editable="false" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_SOURCE_GCL"/>
						            <mx:Button id="BTN_LIST_SOURCE_GCL" selectedField="PODS_SOURCE_GCL" enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100" visible="{CONFIG.SHOW_SYSTEM_INFO}">
					          <mx:Text textAlign="right" text="Идентификатор объекта" width="140"/>
					          <controls:NumberInput id="EVENT_ID" editable="false" width="85" maxChars="22" height="20"/>
				        </mx:HBox>
			      </mx:VBox>
		    </mx:HBox>
	  </mx:VBox>
	  <validatorsArray>
		    <controls:DefaultStringValidator source="{DESCRIPTION}" property="text" enabled="false" minLength="1"
                                       requiredFieldError="Поле 'Описание' обязательно для заполнения."
                                       tooShortError="Поле 'Описание' обязательно для заполнения."/>
		    <controls:DefaultNumberValidator source="{QUANTITY_OF_VENTS}" property="text" enabled="false" required="false"
                                       allowNegative="true"
                                       minLength="0"
                                       requiredFieldError="Поле обязательно для заполнения."
                                       tooShortError="Поле обязательно для заполнения."
                                       precision="0"
                                       precisionError="Значение в поле должно иметь не более 0 цифр после запятой"
                                       maxValue="99999999"
                                       exceedsMaxError="Значение в поле может иметь 8 цифр до запятой и 0 после"
                                       lowerThanMinError="Значение в поле не может быть меньше 0"
                                       negativeError="Значение в поле не может быть меньше 0"/>
		    <controls:DefaultNumberValidator source="{LENGTH}" property="text" enabled="false" required="false"
                                       allowNegative="true"
                                       minLength="0"
                                       requiredFieldError="Поле обязательно для заполнения."
                                       tooShortError="Поле обязательно для заполнения."
                                       precision="0"
                                       precisionError="Значение в поле должно иметь не более 0 цифр после запятой"
                                       maxValue="9999"
                                       exceedsMaxError="Значение в поле может иметь 4 цифр до запятой и 0 после"
                                       lowerThanMinError="Значение в поле не может быть меньше 0"
                                       negativeError="Значение в поле не может быть меньше 0"/>
	  </validatorsArray>
</DATA_EDIT_FORM>