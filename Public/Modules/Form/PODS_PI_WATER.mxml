<?xml version="1.0" encoding="UTF-8"?>
<DATA_EDIT_FORM xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:picPutton="ru.corelight.view.controls.*" xmlns:panel="ru.corelight.grids.buttons.*" xmlns:controls="view.controls.*"
                xmlns="view.data.*"
                title="Грунтовые воды. Данные измерений"
                width="760"
                ext_data_id="PODS_EXPORT.xml#PODS_PI_WATER"
                excelTemplate="PODS_PI_WATER">
	  <dataProvider>PODS_SEM.xml</dataProvider>
   <panel:buttons>
     <picPutton:PanelButton source="../images/catalog.png"
           tooltip="Прикрепленные документы / комментарии"
           clickEvent="DocsEvent" />
     <picPutton:PanelButton source="../images/save.png"
           tooltip="Сохранить"
           clickEvent="SaveEvent" />
     <picPutton:PanelButton source="../images/undo.png"
           tooltip="Отменить изменения"
           clickEvent="UndoEvent" />
     <picPutton:PanelButton source="../images/delete.png"
           tooltip="Удалить"
           clickEvent="DeleteEvent"/>
     <picPutton:PanelButton source="../images/exportNAV.png"
           tooltip="Экспорт в GPX, KML"
           clickEvent="ExportGpxEvent" />
     <picPutton:PanelButton source="../images/showOnMap.png"
           tooltip="Показать на карте"
           clickEvent="ShowOnMapEvent" />
     <picPutton:PanelButton source="../images/find_path.png"
           tooltip="Построение маршрута"
           clickEvent="FindRouteEvent"/>
     <picPutton:PanelButton source="../images/bufferZone.png"
           tooltip="Построить буферную зону"
           clickEvent="BufferCustomEvent"/>
     <picPutton:PanelButton source="../images/excel.png"
           tooltip="Экспорт в Excel"
           clickEvent="ExportToExcelEvent" />
     <picPutton:PanelButton source="../images/back.png"
           tooltip="Вернуться к предыдущему объекту"
           clickEvent="BackEvent"/>
     <picPutton:PanelButton source="../images/forward.png"
           tooltip="Отменить возврат к предыдущему объекту"
           clickEvent="ForwardEvent"/>
   </panel:buttons>
	  <mx:VBox width="760">
		    <mx:HBox percentWidth="100">
			      <mx:Text textAlign="right" text="Обследование" width="140"/>
			      <mx:HBox percentWidth="100" styleName="comboBoxUnit">
				        <mx:ComboBox id="PI_EVENT_ID" width="585" height="20"
                         dataProvider="PODS_SEM_DICT.xml#PODS_PHYSICAL_INSPECTION"/>
				        <mx:Button id="BUTTON_PI_EVENT_ID" selectedField="PODS_PHYSICAL_INSPECTION"
                       enabled="false"
                       styleName="refObject"
                       toolTip="Перейти к обследованию"
                       width="16"
                       height="16"/>
			      </mx:HBox>
		    </mx:HBox>
		    <mx:HBox percentWidth="100">
			      <mx:VBox percentWidth="50">
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Описание" width="140"/>
					          <mx:TextInput id="DESCRIPTION" width="225" maxChars="50" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Дата взятия пробы" width="140"/>
					          <mx:DateField id="INSPECTION_DATE" width="97" height="20" formatString="DD.MM.YYYY"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Номер пробы" width="140"/>
					          <mx:TextInput id="SAMPLE_NUMBER" width="160" maxChars="32" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Местоположение взятия пробы" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="SAMPLE_LOCATION_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_SAMPLE_LOCATION_CL"/>
						            <mx:Button id="BTN_LIST_SAMPLE_LOCATION_CL" selectedField="PODS_SAMPLE_LOCATION_CL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Наличие бактерий" width="140"/>
					          <mx:CheckBox id="BACTERIA_LF" width="20" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Водородный показатель, рH" width="140"/>
					          <controls:NumberInput id="PH" width="30" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Углекислота свободная, мг/дм3" width="140"/>
					          <controls:NumberInput id="CO2" width="30" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Диоксид серы, мг/дм3" width="140"/>
					          <controls:NumberInput id="SO2" width="30" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Сероводород, мг/дм3" width="140"/>
					          <controls:NumberInput id="H2S" width="30" maxChars="22" height="20"/>
				        </mx:HBox>
			      </mx:VBox>
			      <mx:VBox percentWidth="50">
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Карбонаты, мг/дм3" width="140"/>
					          <controls:NumberInput id="CARBONATE" width="30" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Бикарбонаты, мг/дм3" width="140"/>
					          <controls:NumberInput id="BICARBONATE" width="30" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Натрий, мг/дм3" width="140"/>
					          <controls:NumberInput id="NA" width="30" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Калий, мг/дм3" width="140"/>
					          <controls:NumberInput id="K" width="30" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Кальций, мг/дм3" width="140"/>
					          <controls:NumberInput id="CA" width="30" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Магний, мг/дм3" width="140"/>
					          <controls:NumberInput id="MG" width="30" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Комментарии" width="140"/>
					          <mx:TextInput id="COMMENTS" width="225" maxChars="255" height="40"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100" visible="{CONFIG.SHOW_SYSTEM_INFO}">
					          <mx:Text textAlign="right" text="Источник информации" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="SOURCE_GCL" enabled="false" editable="false" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_SOURCE_GCL"/>
						            <mx:Button id="BTN_LIST_SOURCE_GCL" selectedField="PODS_SOURCE_GCL" enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100" visible="{CONFIG.SHOW_SYSTEM_INFO}">
					          <mx:Text textAlign="right" text="Идентификатор объекта" width="140"/>
					          <controls:NumberInput id="EVENT_ID" editable="false" width="85" maxChars="22" height="20"/>
				        </mx:HBox>
			      </mx:VBox>
		    </mx:HBox>
	  </mx:VBox>
	  <validatorsArray>
		    <controls:DefaultStringValidator source="{DESCRIPTION}" property="text" enabled="false" minLength="1"
                                       requiredFieldError="Поле 'Описание' обязательно для заполнения."
                                       tooShortError="Поле 'Описание' обязательно для заполнения."/>
		    <controls:DefaultDateValidator source="{INSPECTION_DATE}" property="selectedDate" enabled="false"
                                     mахValue="new Date()"
                                     exceedsMaxError="'Дата измерения' должна быть не позднее текущей даты"
                                     requiredFieldError="Поле 'Дата измерения' обязательно для заполнения"/>
