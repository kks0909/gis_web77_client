<?xml version="1.0" encoding="UTF-8"?>
<DATA_EDIT_FORM xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:picPutton="ru.corelight.view.controls.*" xmlns:panel="ru.corelight.grids.buttons.*" xmlns:controls="view.controls.*" xmlns="view.data.*" width="760">
  <insertIfEmpty>true</insertIfEmpty>
  <dataProvider>GAZPROM_SYS_SEM.xml</dataProvider>
   <panel:buttons>
     <picPutton:PanelButton source="../images/catalog.png"
           tooltip="Прикрепленные документы / комментарии"
           clickEvent="DocsEvent" />
     <picPutton:PanelButton source="../images/back.png"
           tooltip="Вернуться к предыдущему объекту"
           clickEvent="BackEvent"/>
     <picPutton:PanelButton source="../images/forward.png"
           tooltip="Отменить возврат к предыдущему объекту"
           clickEvent="ForwardEvent"/>
   </panel:buttons>
	<mx:VBox width="760">
		<mx:HBox percentWidth="100">
			<mx:VBox percentWidth="50">
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Описание" width="140"/>
					<mx:TextInput id="DESCRIPTION" width="225" maxChars="50" height="40"/>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Наименование" width="140"/>
					<mx:TextInput id="NAME" width="225" maxChars="255" height="20"/>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Модель" width="140"/>
					<mx:TextInput id="MODEL_NUMBER" width="160" maxChars="32" height="20"/>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Тип корпуса УКЗ/УДЗ" width="140"/>
					<mx:HBox percentWidth="100" styleName="comboBoxUnit">
						<mx:ComboBox id="TYPE_CL" width="200" height="20" dataProvider="PODS_SEM_DICT.xml#PODS_RECTIFIER_ENCLOSURE_TYPE_CL"/>
						<mx:Button id="BTN_LIST_TYPE_CL" selectedField="PODS_RECTIFIER_ENCLOSURE_TYPE_CL" enabled="false" styleName="refList" toolTip="Перейти к редактированию справочника" width="16" height="16"/>
					</mx:HBox>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Компания-изготовитель" width="140"/>
					<mx:HBox percentWidth="100" styleName="comboBoxUnit">
						<mx:ComboBox id="MANUFACTURER_CL" width="200" height="20" dataProvider="PODS_SEM_DICT.xml#PODS_RECTIFIER_ENCLOSURE_MFGR_CL"/>
						<mx:Button id="BTN_LIST_MANUFACTURER_CL" selectedField="PODS_RECTIFIER_ENCLOSURE_MFGR_CL" enabled="false" styleName="refList" toolTip="Перейти к редактированию справочника" width="16" height="16"/>
					</mx:HBox>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Тип источника электроэнергии" width="140"/>
					<mx:HBox percentWidth="100" styleName="comboBoxUnit">
						<mx:ComboBox id="POWER_BILLING_SOURCE_CL" width="200" height="20" dataProvider="PODS_SEM_DICT.xml#PODS_RECTIFIER_POWER_BILL_SOURCE_CL"/>
						<mx:Button id="BTN_LIST_POWER_BILLING_SOURCE_CL" selectedField="PODS_RECTIFIER_POWER_BILL_SOURCE_CL" enabled="false" styleName="refList" toolTip="Перейти к редактированию справочника" width="16" height="16"/>
					</mx:HBox>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Типоразмер основного кабеля" width="140"/>
					<mx:HBox percentWidth="100" styleName="comboBoxUnit">
						<mx:ComboBox id="NEG_CABLE_GAUGE_GCL" width="200" height="20" dataProvider="PODS_SEM_DICT.xml#PODS_CABLE_GAUGE_GCL"/>
						<mx:Button id="BTN_LIST_NEG_CABLE_GAUGE_GCL" selectedField="PODS_CABLE_GAUGE_GCL" enabled="false" styleName="refList" toolTip="Перейти к редактированию справочника" width="16" height="16"/>
					</mx:HBox>
				</mx:HBox>
			</mx:VBox>
			<mx:VBox percentWidth="50">
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Тип изоляции основного кабеля" width="140"/>
					<mx:HBox percentWidth="100" styleName="comboBoxUnit">
						<mx:ComboBox id="NEG_CABLE_INSULATION_TYPE_GCL" width="200" height="20" dataProvider="PODS_SEM_DICT.xml#PODS_CABLE_INSULATION_TYPE_GCL"/>
						<mx:Button id="BTN_LIST_NEG_CABLE_INSULATION_TYPE_GCL" selectedField="PODS_CABLE_INSULATION_TYPE_GCL" enabled="false" styleName="refList" toolTip="Перейти к редактированию справочника" width="16" height="16"/>
					</mx:HBox>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Цвет изоляции основного кабеля" width="140"/>
					<mx:HBox percentWidth="100" styleName="comboBoxUnit">
						<mx:ComboBox id="NEG_CABLE_INSULATION_COLOR_GCL" width="200" height="20" dataProvider="PODS_SEM_DICT.xml#PODS_CABLE_INSULATION_COLOR_GCL"/>
						<mx:Button id="BTN_LIST_NEG_CABLE_INSULATION_COLOR_GCL" selectedField="PODS_CABLE_INSULATION_COLOR_GCL" enabled="false" styleName="refList" toolTip="Перейти к редактированию справочника" width="16" height="16"/>
					</mx:HBox>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Дата установки/монтажа" width="140"/>
					<mx:DateField id="DATE_INSTALLED" width="97" height="20" formatString="DD.MM.YYYY"/>
				</mx:HBox>
				<mx:HBox percentWidth="100">
					<mx:Text textAlign="right" text="Комментарии" width="140"/>
					<mx:TextInput id="COMMENTS" width="225" maxChars="255" height="40"/>
				</mx:HBox>
				<mx:HBox percentWidth="100" visible="{CONFIG.SHOW_SYSTEM_INFO}">
					<mx:Text textAlign="right" text="Источник информации" width="140"/>
					<mx:HBox percentWidth="100" styleName="comboBoxUnit">
						<mx:ComboBox id="SOURCE_GCL" enabled="false" editable="false" width="200" height="20" dataProvider="PODS_SEM_DICT.xml#PODS_SOURCE_GCL"/>
						<mx:Button id="BTN_LIST_SOURCE_GCL" selectedField="PODS_SOURCE_GCL" enabled="false" styleName="refList" toolTip="Перейти к редактированию справочника" width="16" height="16"/>
					</mx:HBox>
				</mx:HBox>
				<mx:HBox percentWidth="100" visible="{CONFIG.SHOW_SYSTEM_INFO}">
					<mx:Text textAlign="right" text="Идентификатор корпуса УКЗ/УДЗ" width="140"/>
					<mx:TextInput id="RECTIFIER_ENCLOSURE_ID" editable="false" width="85" maxChars="16" height="20"/>
				</mx:HBox>
				<mx:HBox percentWidth="100" visible="{CONFIG.SHOW_SYSTEM_INFO}">
					<mx:Text textAlign="right" text="Идентификатор объекта" width="140"/>
					<controls:NumberInput id="EVENT_ID" editable="false" width="85" maxChars="22" height="20"/>
				</mx:HBox>
			</mx:VBox>
		</mx:HBox>
	</mx:VBox>
	<validatorsArray/>
</DATA_EDIT_FORM>
