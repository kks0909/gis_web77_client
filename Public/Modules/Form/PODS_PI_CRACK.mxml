	  <mx:VBox width="760">
		    <mx:HBox percentWidth="100">
			      <mx:Text textAlign="right" text="Обследование" width="140"/>
			      <mx:HBox percentWidth="100" styleName="comboBoxUnit">
				        <mx:ComboBox id="PI_EVENT_ID" width="585" height="20"
                         dataProvider="PODS_SEM_DICT.xml#PODS_PHYSICAL_INSPECTION"/>
				        <mx:Button id="BUTTON_PI_EVENT_ID" selectedField="PODS_PHYSICAL_INSPECTION"
                       enabled="false"
                       styleName="refObject"
                       toolTip="Перейти к обследованию"
                       width="16"
                       height="16"/>
			      </mx:HBox>
		    </mx:HBox>
		    <mx:HBox percentWidth="100">
			      <mx:VBox percentWidth="50">
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Описание" width="140"/>
					          <mx:TextInput id="DESCRIPTION" width="200" maxChars="50" height="40"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Дата измерения" width="140"/>
					          <mx:DateField id="INSPECTION_DATE" width="97" height="20" formatString="DD.MM.YYYY"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Тип трещины" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="TYPE_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_CRACK_TYPE_CL"/>
						            <mx:Button id="BTN_LIST_TYPE_CL" selectedField="PODS_CRACK_TYPE_CL" enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Метод измерения толщины стенки" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="WT_MEASUREMENT_METHOD_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_WT_MEASUREMENT_METHOD_CL"/>
						            <mx:Button id="BTN_LIST_WT_MEASUREMENT_METHOD_CL"
                             selectedField="PODS_WT_MEASUREMENT_METHOD_CL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Метод измерения трещин" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="MEASUREMENT_METHOD_CL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_MEASUREMENT_METHOD_CL"/>
						            <mx:Button id="BTN_LIST_MEASUREMENT_METHOD_CL" selectedField="PODS_MEASUREMENT_METHOD_CL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Категория предельного состояния" width="140"/>
					          <mx:HBox percentWidth="100" styleName="comboBoxUnit">
						            <mx:ComboBox id="FAILURE_ALGORITHM_GCL" width="200" height="20"
                               dataProvider="PODS_SEM_DICT.xml#PODS_FAILURE_ALGORITHM_GCL"/>
						            <mx:Button id="BTN_LIST_FAILURE_ALGORITHM_GCL" selectedField="PODS_FAILURE_ALGORITHM_GCL"
                             enabled="false"
                             styleName="refList"
                             toolTip="Перейти к редактированию справочника"
                             width="16"
                             height="16"/>
					          </mx:HBox>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="На продольном шве" width="140"/>
					          <mx:CheckBox id="LINEAR_AT_WELD_TOE_FLAG_LF" width="20" height="20"/>
					          <mx:Text textAlign="right" text="Разветвление" width="173"/>
					          <mx:CheckBox id="INTERLINKING_LF" width="20" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Критическая трещина" width="140"/>
					          <mx:CheckBox id="SIGNIFICANT_CRACK_LF" width="20" height="20"/>
