					          <mx:CheckBox id="CORROSION_PRESENT_LF" width="20" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Метод металлографии" width="140"/>
					          <mx:TextInput id="METALOGRAPHY" width="100" maxChars="20" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Предельное давление, КПа" width="140"/>
					          <controls:NumberInput id="FAILURE_PRESSURE" width="40" maxChars="22" height="20"/>
				        </mx:HBox>
			      </mx:VBox>
			      <mx:VBox percentWidth="50">
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Максимальная глубина, мм" width="140"/>
					          <controls:NumberInput id="MAX_DEPTH" width="40" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Средняя глубина, мм" width="120"/>
					          <controls:NumberInput id="AVG_DEPTH" width="40" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Длина по оси трубы, м" width="140"/>
					          <controls:NumberInput id="AXIAL_LENGTH" width="40" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Длина в развертке, м" width="120"/>
					          <controls:NumberInput id="CIRCUM_LENGTH" width="40" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Ширина в развертке, м" width="140"/>
					          <controls:NumberInput id="MAX_WIDTH" width="40" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Расстояние между концами трещины, мм" width="140"/>
					          <controls:NumberInput id="TIP_TO_TIP_DIST" width="40" maxChars="22" height="20"/>
					          <mx:Text textAlign="right" text="Ширина раскрытия, мм" width="120"/>
					          <controls:NumberInput id="SIDE_TO_SIDE_SEPARATION" width="40" maxChars="22" height="20"/>
				        </mx:HBox>
				        <mx:HBox percentWidth="100">
					          <mx:Text textAlign="right" text="Отношение раскрытия к толщине стенки" width="140"/>
