  <Document>
	<!-- Стиль линии для подписей на карте легенды (линия прозрачная и белая, чтобы была видна только подпись) -->
	<Style id="CAS_LEGEND" comment="CAS.LEGEND">
		<zoomMin>NAN</zoomMin>
		<zoomMax>NAN</zoomMax>
		<LabelStyle>
			<zoomMin>NaN</zoomMin>
			<zoomMax>NaN</zoomMax>
			<field>LABEL</field>
			<color>FF404040</color>
			<fontSize>10</fontSize>
			<bold>0</bold>
			<italic>0</italic>
			<underline>0</underline>
			<parallel>1</parallel>
			<auto>1</auto>
			<offset>0</offset>
			<duplicate>0</duplicate>
			<overlap>1</overlap>
      <align>right|bottom</align>
		</LabelStyle>
		<LineStyle>
			<color>00FFFFFF</color>
			<width>1</width>
			</LineStyle>
	</Style>
	<!-- Стиль для единиц измерения на карте и легенде -->
	<Style id="CAS_UNITS" comment="CAS.UNITS">
		<zoomMin>NAN</zoomMin>
		<zoomMax>NAN</zoomMax>
		<LabelStyle>
			<zoomMin>NaN</zoomMin>
			<zoomMax>NaN</zoomMax>
			<field>LABEL</field>
			<color>7D000000</color>
			<fontSize>10</fontSize>
			<bold>0</bold>
			<italic>1</italic>
			<underline>0</underline>
			<parallel>1</parallel>
			<auto>1</auto>
			<offset>0</offset>
			<duplicate>0</duplicate>
			<overlap>1</overlap>
		</LabelStyle>
		<LineStyle>
			<color>00FFFFFF</color>
			<width>1</width>
			</LineStyle>
	</Style>
	<!-- Стиль линии для осей на карте диаграмм и на карте легенды (линия серая, чтобы не мешалась) -->
	<Style id="CAS_AXES" schema="CAS" table="AXES" comment="CAS.AXES">
      <zoomMin>NaN</zoomMin>
      <zoomMax>NaN</zoomMax>
      <LineStyle>
      	<color>FF000000</color>
      	<width>2</width>
      </LineStyle>
      <LabelStyle>
        <zoomMin>NaN</zoomMin>
        <zoomMax>NaN</zoomMax>
        <field>LABEL</field>
        <color>FF000000</color>
        <fontSize>10</fontSize>
        <bold>0</bold>
        <italic>0</italic>
        <underline>0</underline>
        <parallel>1</parallel>
        <auto>1</auto>
        <offset>0</offset>
        <duplicate>1</duplicate>
        <overlap>0</overlap>
      </LabelStyle>
    </Style>
	<!-- Стиль линии для вертикальных разделительных осей на карте диаграмм и на карте легенды (линия свето-серая, чтобы не мешалась) -->
