      <zoomMin>NaN</zoomMin>
      <zoomMax>NaN</zoomMax>
      <LineStyle>
         <color>FFC0C0C0</color>
	       <width>1</width>
      </LineStyle>
      <LabelStyle>
        <zoomMin>NaN</zoomMin>
        <zoomMax>NaN</zoomMax>
        <field>LABEL</field>
        <color>FF000000</color>
        <fontSize>10</fontSize>
        <bold>0</bold>
        <italic>0</italic>
        <underline>0</underline>
        <parallel>1</parallel>
        <auto>1</auto>
        <offset>0</offset>
        <duplicate>1</duplicate>
        <overlap>0</overlap>
      </LabelStyle>
    </Style>
