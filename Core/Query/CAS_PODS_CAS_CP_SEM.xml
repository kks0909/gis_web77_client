      <dbQuery idField="ID">
        <var name="FILTER" default="1=1" />
        <query>SELECT * FROM (SELECT EVENT_ID ID, t.* FROM PODS.PI_CP_INSPECTION t) d WHERE ID::TEXT like  substr('{ID}',0,strpos('{ID}','-')) and {FILTER}</query>
      </dbQuery>
    </select>
